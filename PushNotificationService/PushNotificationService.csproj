    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>7c9dcfe3-c741-439a-b8d3-003e38e9f8e0</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
